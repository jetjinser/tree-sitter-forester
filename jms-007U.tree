\import{base-macros}
\import{jms-007T}

\title{propositions do not notice set truncation of indices}
\author{jonmsterling}
\date{2023-08-18}
\taxon{lemma}

\let\P{#{P}}

\p{Any proposition is orthogonal to the map #{\ixsum{x:A}B\ToTrunc{0}{x}\to \ixsum{x:\Trunc{0}{A}}Bx} sending \prn{x,y} to \prn{\ToTrunc{0}{x},y}.}

\proof{
  \p{This follows from the dependent induction principle of set-truncation, as any proposition is a set.}
}
